    </AnchorPane>
    <AnchorPane prefHeight="50.0">
        <Label alignment="CENTER" text="会员登记" textFill="WHITE" AnchorPane.bottomAnchor="0.0"
               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
